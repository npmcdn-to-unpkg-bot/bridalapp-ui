{
  "name": "bridalapp-ui",
  "version": "0.1.0",
  "description": "User Interface for BridalApp",
  "keywords": [
    "OpenShift",
    "Node.js",
    "BridalApp"
  ],
  "main": "server.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "homepage": "http://www.bridalapp.net/",
  "repository": {
    "type": "git",
    "url": "https://github.com/download/bridalapp-ui"
  },
  "scripts": {
    "clean-server": "rimraf hmr && rimraf server.js",
    "clean-client": "rimraf public/assets/bridalapp-ui.js",
    "clean": "npm run clean-server && npm run clean-client",
    "start": "node server.js",
    "start-dev": "node ./node_modules/just-wait/bin/just-wait.js --pattern \"server.js\" && npm run start",
    "build-server": "webpack --config webpack/server.config.js",
    "build-client": "webpack --config webpack/client.config.js",
    "build": "npm run build-server && npm run build-client",
    "release-server": "npm run clean-server && npm run build-server",
    "release-client": "npm run clean-client && npm run build-client",
    "release": "npm run clean && npm run build",
    "watch": "webpack --inline --watch --hide-modules --config webpack/development.server.config.js",
    "dev": "npm run -s clean && parallelshell \"npm run -s watch\" \"npm run -s start-dev\"",
    "test": "mocha --watch --require babel-register \"src/**/*.spec.jsx\"",
    "test-dev": "webpack-dev-server --inline --hot --config webpack/test.config.js --port 8081"
  },
  "engines": {
    "node": ">= 0.6.0",
    "npm": ">= 1.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babel-runtime": "^6.3.19",
    "chalk": "^1.1.1",
    "classnames": "^2.2.3",
    "express": "^4.0.0",
    "fs": "0.0.2",
    "history": "^1.17.0",
    "http-proxy": "^1.12.0",
    "isomorphic-fetch": "^2.2.1",
    "picolog": "^1.0.0",
    "react": "^0.14.6",
    "react-document-meta": "^2.0.1",
    "react-dom": "^0.14.6",
    "react-mdl": "^1.1.0",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-apis": "^0.9.2",
    "redux-simple-router": "^2.0.3",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "app-root-path": "^1.0.0",
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "eventsource-polyfill": "^0.9.6",
    "just-wait": "^1.0.3",
    "mocha": "^2.3.4",
    "mocha-loader": "^0.7.1",
    "parallelshell": "^2.0.0",
    "picolog": "^1.0.0",
    "react-addons-test-utils": "^0.14.6",
    "react-mdl": "^1.0.4",
    "react-shallow-testutils": "^0.7.1",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.5.0",
    "source-map-support": "^0.4.0",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.0"
  },
  "author": {
    "name": "Stijn de Witt",
    "email": "StijnDeWitt@hotmail.com",
    "url": "http://www.StijnDeWitt.com/"
  },
  "copyright": "2016 by Stijn de Witt, some rights reserved.",
  "license": "CC-BY-4.0",
  "licenseUrl": "https://creativecommons.org/licenses/by/4.0/"
}
